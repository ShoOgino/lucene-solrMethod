  /**
   * Returns a normalized Lng rectangle shape for the bounding box
   */
  private static LatLng normLng(double lat,double lng) {
    if (lng > 180.0) {
        lng = -1.0*(180.0 - (lng - 180.0));
    }
    else if (lng < -180.0) {
        lng = (lng + 180.0)+180.0;
    }
    LatLng ll=new FloatLatLng(lat, lng);
    return ll;
  }

