    /**
     * the timestamp of the last modification for the given feed id
     * 
     * @param feedId -
     *            feed id
     * @return timestamp
     */
    protected Long getFeedLastModified(final String feedId) {
        this.readLock.lock();
        try {
            return this.modifiyMap.get(feedId);
        } finally {
            this.readLock.unlock();
        }
    }

