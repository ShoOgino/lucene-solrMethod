    /*
     * Test method for
     * 'org.apache.lucene.gdata.server.registry.configuration.ConfigurationBean.set(String,
     * String)'
     */
    public void testSet() {
        this.bean.set("field", "value");
        try {
            this.bean.set("field", "value");
            fail("field already set");
        } catch (IllegalArgumentException e) {
            //
        }
        assertEquals("value", this.bean.get("field"));
    }

