    /**
     * @see java.lang.Object#equals(java.lang.Object)
     */
    @Override
    public boolean equals(Object object) {
        if (this == object)
            return true;
        if (object == null)
            return false;
        if (object instanceof IndexSchema) {
           if(this.name ==null)
               return super.equals(object);
            return this.name.equals(((IndexSchema) object).getName());
        }
        return false;
    }

