    @SuppressWarnings("unchecked") 
    private BaseEntry addcontextPath(final BaseEntry entry, 
            final String contextPath) { 
        String id = contextPath + entry.getId(); 
        entry.setId(id); 
        Link self = new Link(); 
        self.setRel("self"); 
        self.setHref(id); 
        self.setType("application/atom+xml"); 
        entry.getLinks().add(self); 
        return entry; 
    } 

