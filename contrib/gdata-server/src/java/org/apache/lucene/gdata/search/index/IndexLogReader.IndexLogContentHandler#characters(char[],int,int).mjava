        /**
         * @see org.xml.sax.ContentHandler#characters(char[], int, int)
         */
        public void characters(char[] ch, int start, int length)
                throws SAXException {
            if(this.isId)
                this.currentID = new String(ch,start,length);
            if(this.isAction)
                this.currentAction = new String(ch,start,length);
            
        }

