  public void add(Token t) {
    //Check to see if this token is a date
    if (t != null) {
      try {
        Date date = dateFormat.parse(t.term());//We don't care about the date, just that we can parse it as a date
        if (date != null) {
          t.setType(DATE_TYPE);
          super.add(t);
        }
      } catch (ParseException e) {

      }
    }

  }

