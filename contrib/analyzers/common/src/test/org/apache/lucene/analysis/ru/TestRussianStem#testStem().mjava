    public void testStem()
    {
        for (int i = 0; i < words.size(); i++)
        {
            //if ( (i % 100) == 0 ) System.err.println(i);
            String realStem =
                RussianStemmer.stemWord(
                    (String) words.get(i));
            assertEquals("unicode", stems.get(i), realStem);
        }
    }

