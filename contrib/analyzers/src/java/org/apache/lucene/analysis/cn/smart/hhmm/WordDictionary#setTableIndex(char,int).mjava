  /*
   * 计算字符c在哈希表中应该在的位置，然后将地址列表中该位置的值初始化
   * 
   */
  private boolean setTableIndex(char c, int j) {
    int index = getAvaliableTableIndex(c);
    if (index != -1) {
      charIndexTable[index] = c;
      wordIndexTable[index] = (short) j;
      return true;
    } else
      return false;
  }

