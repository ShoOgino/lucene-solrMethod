    Long stringResult = (Long)getStatResult("ur", "string_sd", VAL_TYPE.LONG);
    Long stringTest = (Long)calculateStat(stringTestStart, "unique");
    assertEquals(getRawResponse(), stringResult,stringTest);
  }

