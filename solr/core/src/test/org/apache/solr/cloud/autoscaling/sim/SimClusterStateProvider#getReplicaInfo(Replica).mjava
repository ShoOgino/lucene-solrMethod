  private ReplicaInfo getReplicaInfo(Replica r) {
    List<ReplicaInfo> list = nodeReplicaMap.get(r.getNodeName());
    if (list == null) {
      return null;
    }
    for (ReplicaInfo ri : list) {
      if (r.getName().equals(ri.getName())) {
        return ri;
      }
    }
    return null;
  }

