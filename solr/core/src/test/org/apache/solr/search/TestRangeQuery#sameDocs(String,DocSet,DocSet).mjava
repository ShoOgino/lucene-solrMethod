  static boolean sameDocs(String msg, DocSet a, DocSet b) {
    DocIterator i = a.iterator();
    // System.out.println("SIZES="+a.size() + "," + b.size());
    assertEquals(msg, a.size(), b.size());
    while (i.hasNext()) {
      int doc = i.nextDoc();
      assertTrue(msg, b.exists(doc));
      // System.out.println("MATCH! " + doc);
    }
    return true;
  }

