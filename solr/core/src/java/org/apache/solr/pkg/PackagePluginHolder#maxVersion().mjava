  private String maxVersion() {
    RequestParams.ParamSet p = core.getSolrConfig().getRequestParams().getParams(PackageListeners.PACKAGE_VERSIONS);
    if (p == null) {
      return null;
    }
    Object o = p.get().get(info.pkgName);
    if (o == null || LATEST.equals(o)) return null;
    return o.toString();
  }

