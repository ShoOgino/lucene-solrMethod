  @Override
  protected void init(IndexSchema schema, Map<String, String> args) {
    super.init(schema, args);

    String v = args.remove("numberType");
    if (v != null) {
      numberFieldName = v;
    }

    v = args.remove("booleanType");
    if (v != null) {
      booleanFieldName = v;
    }
  }

