  ElevationObj getElevationObj(String query, String[] ids, String[] ex) throws IOException {
    if (ids == null) {
      ids = new String[0];
    }
    if (ex == null) {
      ex = new String[0];
    }

    ElevationObj obj = new ElevationObj(query, Arrays.asList(ids), Arrays.asList(ex));
    return obj;
  }

