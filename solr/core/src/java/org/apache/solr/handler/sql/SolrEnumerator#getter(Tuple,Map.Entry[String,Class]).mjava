  private Object getter(Tuple tuple, Map.Entry<String, Class> field) {
    Object val = tuple.get(field.getKey());

    if(val == null) {
      return null;
    }

    Class clazz = field.getValue();
    if(clazz.equals(Long.class)) {
      if(val instanceof Double) {
        return this.getRealVal(val);
      }
      return val;
    }

    return val;
  }

