  private static Object changeChildDoc(Object o) {
    if (o instanceof List) {
      return ((List) o)
          .stream()
          .map(JsonLoader::changeChildDoc)
          .collect(toList());
    }
    Map m = (Map) o;
    if (m.containsKey(null)) m.put(CHILD_DOC_KEY, changeChildDoc(m.remove(null)));
    return m;
  }

