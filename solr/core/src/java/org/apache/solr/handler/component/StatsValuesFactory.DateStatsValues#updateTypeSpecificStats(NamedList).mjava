    @Override
    protected void updateTypeSpecificStats(@SuppressWarnings({"rawtypes"})NamedList stv) {
      if (computeSum) {
        sum += ((Number) stv.get("sum")).doubleValue();
      }
      if (computeSumOfSquares) {
        sumOfSquares += ((Number) stv.get("sumOfSquares")).doubleValue();
      }
    }

