  @Override
  @SuppressWarnings({"unchecked", "rawtypes"})
  public Collection pickSubset(Collection values) {
    Set<Object> uniqs = new HashSet<>();
    List<Object> result = new ArrayList<>(values.size());
    for (Object o : values) {
      if (!uniqs.contains(o)) {
        uniqs.add(o);
        result.add(o);
      }
    }
    return result;
  }

