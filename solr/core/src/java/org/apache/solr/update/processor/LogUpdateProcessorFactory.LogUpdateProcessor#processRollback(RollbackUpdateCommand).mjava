    /**
     * @since Solr 1.4
     */
    @Override
    public void processRollback( RollbackUpdateCommand cmd ) throws IOException {
      if (logDebug) {
        log.debug("PRE_UPDATE {}", cmd, req);
      }
      if (next != null) next.processRollback(cmd);

      toLog.add("rollback", "");
    }

