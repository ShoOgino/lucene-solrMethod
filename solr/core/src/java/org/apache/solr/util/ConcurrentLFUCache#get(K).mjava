  @Override
  public V get(K key) {
    CacheEntry<K, V> e = map.get(key);
    if (e == null) {
      if (islive) stats.missCounter.increment();
    } else if (islive) {
      e.lastAccessed = timeSource.getEpochTimeNs();
      stats.accessCounter.increment();
      e.hits.increment();
    }
    return e != null ? e.value : null;
  }

