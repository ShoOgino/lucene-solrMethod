  @Override
  public String toString() {
    return "AggregateMetric{" +
        "size=" + size() +
        ", max=" + getMax() +
        ", min=" + getMin() +
        ", mean=" + getMean() +
        ", stddev=" + getStdDev() +
        ", sum=" + getSum() +
        ", values=" + values +
        '}';
  }

