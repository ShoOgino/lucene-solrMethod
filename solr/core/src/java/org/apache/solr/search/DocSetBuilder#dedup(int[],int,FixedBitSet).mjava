  private static int dedup(int[] arr, int length, FixedBitSet acceptDocs) {
    if (length == 0) {
      return 0;
    }
    int l = 1;
    int previous = arr[0];
    for (int i = 1; i < length; ++i) {
      final int value = arr[i];
      assert value >= previous;
      if (value != previous) {
        if (acceptDocs == null || acceptDocs.get(value)) {
          arr[l++] = value;
          previous = value;
        }
      }
    }
    return l;
  }

