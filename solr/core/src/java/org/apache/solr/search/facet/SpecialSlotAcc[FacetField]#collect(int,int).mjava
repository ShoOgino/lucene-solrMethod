  @Override
  public void collect(int doc, int slot) throws IOException {
    assert slot != collectAccSlot || slot < 0;
    count++;
    if (collectAcc != null) {
      collectAcc.collect(doc, collectAccSlot);
    }
    if (otherAccs != null) {
      for (SlotAcc otherAcc : otherAccs) {
        otherAcc.collect(doc, otherAccsSlot);
      }
    }
  }

