  @Override
  public DocIterator iterator() {
    return new DocIterator() {
      int pos=0;
      int doc;
      { goNext(); }

      @Override
      public boolean hasNext() {
        return pos < table.length;
      }

      @Override
      public Integer next() {
        return nextDoc();
      }

      @Override
      public void remove() {
      }

      void goNext() {
        while (pos<table.length && table[pos]==EMPTY) pos++;
      }

      // modify to return -1 at end of iteration?
      @Override
      public int nextDoc() {
        int doc = table[pos];
        pos++;
        goNext();
        return doc;
      }

      @Override
      public float score() {
        return 0.0f;
      }
    };
  }

