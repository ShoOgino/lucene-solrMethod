  @Override
  public void collect(int doc, int slotNum, IntFunction<SlotContext> slotContext) throws IOException {
    if (doc > subDv.docID()) {
      subDv.advance(doc);
    }
    if (doc == subDv.docID()) {
      int segOrd = subDv.ordValue();
      int ord = toGlobal==null ? segOrd : (int)toGlobal.get(segOrd);

      collectOrdToSlot(slotNum, ord);
    }
  }

