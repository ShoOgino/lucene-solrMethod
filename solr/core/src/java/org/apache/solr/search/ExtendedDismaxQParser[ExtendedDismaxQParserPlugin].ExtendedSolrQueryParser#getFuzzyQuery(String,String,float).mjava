    @Override
    protected Query getFuzzyQuery(String field, String val, float minSimilarity) throws SyntaxError {
//System.out.println("getFuzzyQuery: val="+val);

      this.type = QType.FUZZY;
      this.field = field;
      this.val = val;
      this.flt = minSimilarity;
      return getAliasedQuery();
    }

