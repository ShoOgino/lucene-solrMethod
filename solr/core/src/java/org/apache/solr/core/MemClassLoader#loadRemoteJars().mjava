  synchronized void loadRemoteJars() {
    if (allJarsLoaded) return;
    int count = 0;
    for (RuntimeLib lib : libs) {
      if (lib.getUrl() != null) {
        try {
          lib.loadJar();
          lib.verify();
        } catch (Exception e) {
          log.error("Error loading runtime library", e);
        }
        count++;
      }
    }
    if (count == libs.size()) allJarsLoaded = true;
  }

