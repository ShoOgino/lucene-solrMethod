  /**
   * Closes the underlying ZooKeeper client.
   */
  public void close() {
    
    for (ElectionContext context : electionContexts.values()) {
      context.close();
    }
    
    try {
      overseer.close();
    } catch(Throwable t) {
      log.error("Error closing overseer", t);
    }
    zkClient.close();
  }

