  public boolean offerEvent(TriggerEvent event) {
    event.getProperties().put(ENQUEUE_TIME, timeSource.getTimeNs());
    try {
      byte[] data = Utils.toJSON(event);
      delegate.offer(data);
      return true;
    } catch (Exception e) {
      log.warn("Exception adding event {} to queue {}", event, triggerName, e);
      return false;
    }
  }

