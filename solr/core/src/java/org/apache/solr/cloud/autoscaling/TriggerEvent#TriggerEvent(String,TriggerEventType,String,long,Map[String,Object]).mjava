  public TriggerEvent(String id, TriggerEventType eventType, String source, long eventTime,
                      Map<String, Object> properties) {
    this.id = id;
    this.eventType = eventType;
    this.source = source;
    this.eventTime = eventTime;
    if (properties != null) {
      this.properties.putAll(properties);
    }
  }

