    public boolean contains(Object o) {
      if (!(o instanceof Map.Entry))
        return false;
      Map.Entry e = (Map.Entry)o;
      Object key = e.getKey();
      if (key==null) return false;  // we don't support null keys
      Object val = e.getValue();
      Object v = get(key);
      return v==null ? val==null : v.equals(val);
    }

