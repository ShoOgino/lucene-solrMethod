    @Override
    protected Query getRangeQuery(String field, String a, String b, boolean inclusive) throws ParseException {
//System.out.println("getRangeQuery:");

      this.type = QType.RANGE;
      this.field = field;
      this.val = a;
      this.val2 = b;
      this.bool = inclusive;
      return getAliasedQuery();
    }

