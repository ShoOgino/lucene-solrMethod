    @Override
    public void copy(int slot, int doc) {
      final int ord = termsIndex.getOrd(doc);
      assert ord >= 0;
      if (ord == 0) {
        ords[slot] = NULL_ORD;
        values[slot] = null;
      } else {
        ords[slot] = ord;
        if (values[slot] == null) {
          values[slot] = new BytesRef();
        }
        termsIndex.lookup(ord, values[slot]);
      }
      readerGen[slot] = currentReaderGen;
    }

