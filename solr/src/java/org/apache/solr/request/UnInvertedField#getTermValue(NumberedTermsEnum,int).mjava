  /** may return a reused BytesRef */
  BytesRef getTermValue(NumberedTermsEnum te, int termNum) throws IOException {
    if (bigTerms.size() > 0) {
      // see if the term is one of our big terms.
      TopTerm tt = bigTerms.get(termNum);
      if (tt != null) {
        return tt.term;
      }
    }

    return te.skipTo(termNum);
  }

