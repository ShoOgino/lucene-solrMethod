  static boolean sameDocs(DocSet a, DocSet b) {
    DocIterator i = a.iterator();
    // System.out.println("SIZES="+a.size() + "," + b.size());
    assertEquals(a.size(), b.size());
    while (i.hasNext()) {
      int doc = i.nextDoc();
      if (!b.exists(doc)) {
        TestCase.fail("Missing doc " + doc);
      }
      // System.out.println("MATCH! " + doc);
    }
    return true;
  }

