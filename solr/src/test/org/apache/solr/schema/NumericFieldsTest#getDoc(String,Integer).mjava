  public static SolrInputDocument getDoc( String id, Integer number )
  {
    SolrInputDocument doc = new SolrInputDocument();
    doc.addField( "id", id );
    for( String t : types ) {
      doc.addField( t, number );
      doc.addField( t+"_last", number );
      doc.addField( t+"_first", number );
    }
    return doc;
  }

