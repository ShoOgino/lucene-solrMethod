    exists = false;
    while (++i < params.length) {
      value = params[i].getDouble();
      exists = params[i].exists();
      if (exists) {
        break;
      }
    }
    while (++i < params.length) {
      temp = params[i].getDouble();
      if (params[i].exists()) {
