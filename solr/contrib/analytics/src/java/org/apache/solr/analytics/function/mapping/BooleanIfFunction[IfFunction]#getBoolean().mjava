  @Override
  public boolean getBoolean() {
    exists = false;
    boolean value = false;
    boolean ifValue = ifExpr.getBoolean();
    if (ifExpr.exists()) {
      if (ifValue) {
        value = thenExpr.getBoolean();
        exists = thenExpr.exists();
      }
      else {
        value = elseExpr.getBoolean();
        exists = elseExpr.exists();
      }
    }
    return value;
  }

