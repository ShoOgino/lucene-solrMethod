  @Override
  public String getString() {
    int i = -1;
    String value = null;
    exists = false;
    while (++i < params.length) {
      value = params[i].getString();
      exists = params[i].exists();
      if (exists) {
        break;
      }
    }
    while (++i < params.length) {
      temp = params[i].getString();
      if (params[i].exists()) {
        value = lambda.apply(value, temp);
      }
    }
    return value;
  }

