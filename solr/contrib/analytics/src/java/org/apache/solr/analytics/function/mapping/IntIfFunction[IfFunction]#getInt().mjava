  @Override
  public int getInt() {
    exists = false;
    int value = 0;
    boolean ifValue = ifExpr.getBoolean();
    if (ifExpr.exists()) {
      if (ifValue) {
        value = thenExpr.getInt();
        exists = thenExpr.exists();
      }
      else {
        value = elseExpr.getInt();
        exists = elseExpr.exists();
      }
    }
    return value;
  }

