  @Override
  public long getLong() {
    exists = false;
    long value = 0;
    boolean ifValue = ifExpr.getBoolean();
    if (ifExpr.exists()) {
      if (ifValue) {
        value = thenExpr.getLong();
        exists = thenExpr.exists();
      }
      else {
        value = elseExpr.getLong();
        exists = elseExpr.exists();
      }
    }
    return value;
  }

