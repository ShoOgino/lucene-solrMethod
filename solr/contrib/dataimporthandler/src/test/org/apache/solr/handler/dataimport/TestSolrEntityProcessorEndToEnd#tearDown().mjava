  @Override
  @After
  public void tearDown() throws Exception {
    try {
      deleteCore();
    } catch (Exception e) {
      log.error("Error deleting core", e);
    }
    if (null != jetty) {
      jetty.stop();
      jetty = null;
    }
    if (null != instance) {
      instance.tearDown();
      instance = null;
    }
    super.tearDown();
  }

