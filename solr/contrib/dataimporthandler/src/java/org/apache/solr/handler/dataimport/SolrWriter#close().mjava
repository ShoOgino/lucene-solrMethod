  @Override
  public void close() {
    try {
      processor.finish();
    } catch (IOException e) {
      throw new DataImportHandlerException(DataImportHandlerException.SEVERE,
          "Unable to call finish() on UpdateRequestProcessor", e);
    } finally {
      deltaKeys = null;
      try {
        processor.close();
      } catch (IOException e) {
        SolrException.log(log, e);
      }
    }
  }

