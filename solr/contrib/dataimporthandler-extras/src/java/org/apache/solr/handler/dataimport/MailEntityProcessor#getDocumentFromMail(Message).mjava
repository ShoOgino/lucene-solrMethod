  private Map<String,Object> getDocumentFromMail(Message mail) {
    Map<String,Object> row = new HashMap<>();
    try {
      addPartToDocument(mail, row, true);
      return row;
    } catch (Exception e) {
      log.error("Failed to convert message [{}] to document due to: {}"
          , mail, e, e);
      return null;
    }
  }

