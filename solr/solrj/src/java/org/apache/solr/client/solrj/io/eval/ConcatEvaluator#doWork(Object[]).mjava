  @Override
  public Object doWork(Object values[]) throws IOException {

    StringBuilder buff = new StringBuilder();

    for(Object o : values) {
        if(buff.length() > 0) {
          buff.append(delim);
        }
        String s = o.toString();
        if(s.startsWith("\"") && s.endsWith("\"")) {
          s = s.substring(1, s.length()-1);
        }
        buff.append(s.toString());
    }

    return buff.toString();
  }

