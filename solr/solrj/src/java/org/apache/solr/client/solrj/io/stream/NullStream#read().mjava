  public Tuple read() throws IOException {

    if(eof != null) {
      return eof;
    }

    while(true) {
      Tuple tuple  = stream.read();
      if(tuple.EOF) {
        eof = tuple;
        long end = new Date().getTime();
        Tuple t = new Tuple(new HashMap());
        t.put("nullCount", count);
        t.put("timer", end-start);
        return t;
      } else {
        ++count;
      }
    }
  }

