    public KnnRegressionTuple(Matrix observations,
                              double[] outcomes,
                              int k,
                              DistanceMeasure distanceMeasure,
                              Map<?,?> map,
                              boolean scale,
                              boolean robust) {
      super(map);
      this.observations = observations;
      this.outcomes = outcomes;
      this.k = k;
      this.distanceMeasure = distanceMeasure;
      this.scale = scale;
      this.robust = robust;
    }

