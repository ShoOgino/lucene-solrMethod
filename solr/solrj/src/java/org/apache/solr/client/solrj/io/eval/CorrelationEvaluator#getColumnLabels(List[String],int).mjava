  public static List<String> getColumnLabels(List<String> labels, int length) {
    if(labels != null) {
      return labels;
    } else {
      List<String> l = new ArrayList();
      for(int i=0; i<length; i++) {
        String label = "col"+ ZplotStream.pad(Integer.toString(i), length);
        l.add(label);
      }

      return l;
    }
  }

