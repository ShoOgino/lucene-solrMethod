  public Map<String, Object> getProperties()  {
    if (properties == null) {
      if (jsonMap != null)  {
        Map<String, Object> map = (Map<String, Object>) jsonMap.get("properties");
        if (map == null) {
          this.properties = Collections.emptyMap();
        } else  {
          this.properties = new LinkedHashMap<>(map);
        }
      } else  {
        this.properties = Collections.emptyMap();
      }
    }
    return properties;
  }

