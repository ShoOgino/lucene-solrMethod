  protected String[] split(String line) {
    String[] fields = line.split(",(?=([^\"]|\"[^\"]*\")*$)",-1);
    for(int i=0; i<fields.length; i++) {
      String f = fields[i];
      if(f.startsWith("\"") && f.endsWith("\"")) {
        f = f.substring(1, f.length()-1);
        fields[i] = f;
      }
    }

    return fields;
  }

