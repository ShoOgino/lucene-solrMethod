  public static String pad(String v, int length) {
    if(length < 11) {
      return v;
    } else if(length < 101) {
      return prepend(v, 2);
    } else if (length < 1001) {
      return prepend(v, 3);
    } else if(length < 10001){
      return prepend(v, 4);
    } else {
      return prepend(v, 5);
    }
  }

