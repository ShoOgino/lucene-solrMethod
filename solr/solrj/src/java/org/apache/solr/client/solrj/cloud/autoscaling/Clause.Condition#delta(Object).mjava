    public Long delta(Object val) {
      if (this.val instanceof String) {
        if (op == LESS_THAN || op == GREATER_THAN) {
          return op.delta(Clause.parseDouble(name, this.val), Clause.parseDouble(name, val));
        } else {
          return 0l;
        }
      } else return op.delta(this.val, val);
    }

