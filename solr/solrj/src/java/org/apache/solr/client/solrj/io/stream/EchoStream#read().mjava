  public Tuple read() throws IOException {

    if(finished) {
      HashMap m = new HashMap();
      m.put("EOF", true);
      Tuple tuple = new Tuple(m);
      return tuple;
    } else {
      HashMap m = new HashMap();
      m.put("echo", echo);
      Tuple tuple = new Tuple(m);
      finished = true;
      return tuple;
    }
  }

