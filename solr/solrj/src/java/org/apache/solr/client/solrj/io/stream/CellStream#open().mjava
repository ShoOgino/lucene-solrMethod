  public void open() throws IOException {
    try {
      stream.open();
      List<Tuple> list = new ArrayList();
      while(true) {
        Tuple tuple = stream.read();
        if(tuple.EOF) {
          EOFTuple = tuple;
          break;
        } else {
          list.add(tuple);
        }
      }

      Map map = new HashMap();
      map.put(name, list);
      tuple = new Tuple(map);
    } finally {
      stream.close();
    }
  }

