  public void open() throws IOException {
    try {
      stream.open();
      List<Tuple> list = new ArrayList();
      while(true) {
        Tuple tuple = stream.read();
        if(tuple.EOF) {
          EOFTuple = tuple;
          break;
        } else {
          list.add(tuple);
        }
      }

      tuple = new Tuple();
      tuple.put(name, list);
    } finally {
      stream.close();
    }
  }

