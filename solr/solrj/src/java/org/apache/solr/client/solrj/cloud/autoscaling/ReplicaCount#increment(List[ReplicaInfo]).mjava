  public void increment(List<ReplicaInfo> infos) {
    if (infos == null) return;
    for (ReplicaInfo info : infos) {
      switch (info.getType()) {
        case NRT:
          nrt++;
          break;
        case PULL:
          pull++;
          break;
        case TLOG:
          tlog++;
          break;
        default:
          nrt++;
      }
    }
  }

