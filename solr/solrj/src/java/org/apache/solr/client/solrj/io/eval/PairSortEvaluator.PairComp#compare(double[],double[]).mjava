    public int compare(double[] a, double[] b) {
      if(a[0] > b[0]) {
        return 1;
      } else if(a[0] < b[0]) {
        return -1;
      } else {
        if(a[1] > b[1]) {
          return 1;
        } else if(a[1] < b[1]){
          return -1;
        } else {
          return 0;
        }
      }
    }

