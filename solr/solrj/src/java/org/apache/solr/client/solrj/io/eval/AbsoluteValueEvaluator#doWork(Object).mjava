  @Override
  public Object doWork(Object value){
    if(null == value){
      return null;
    }
    else if(value instanceof List){
      return ((List<?>)value).stream().map(innerValue -> doWork(innerValue));
    }
    else{
      // we know it's a BigDecimal
      return Math.abs(((BigDecimal)value).doubleValue());
    }
  }

