  /**
   * This method is implemented for tests and should not be counted
   * on in production code.
   * 
   * @lucene.experimental
   */
  @Override
  public int hashCode() {
    int result = name.hashCode();
    result = 31 * result + value.hashCode();
    result = 31 * result + (boost != +0.0f ? Float.floatToIntBits(boost) : 0);
    return result;
  }

