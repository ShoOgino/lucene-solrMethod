    private void putValue(Map<String, Object> values, String fieldName, Object o) {
      if(o==null) return;
      Object val = values.get(fieldName);
      if (val == null) {
        values.put(fieldName, o);
        return;
      }
      if (val instanceof List) {
        List list = (List) val;
        list.add(o);
        return;
      }
      ArrayList l = new ArrayList();
      l.add(val);
      l.add(o);
      values.put(fieldName, l);
    }

