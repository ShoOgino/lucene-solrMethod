  /**
   * @return null if ok else error message
   */
  public static String clearObjectTrackerAndCheckEmpty(int waitSeconds) {
    int retries = 0;
    String result;
    do {
      result = checkEmpty();
      if (result == null)
        break;
      try {
        TimeUnit.SECONDS.sleep(1);
      } catch (InterruptedException e) { break; }
    }
    while (retries++ < waitSeconds);
    
    OBJECTS.clear();
    
    return result;
  }

