  @Override
  public boolean equals(Object o) {
    if (this == o) {
      return true;
    }
    if (!(o instanceof SolrDocument)) {
      return false;
    }

    SolrDocument solrDocument = (SolrDocument) o;

    if (!_fields.equals(solrDocument._fields)) {
