    @Override
    public void writeMap(EntryWriter ew) throws IOException {
      ew.put(name, (MapWriter) ew1 -> {
        if(variables !=null){
          for (Map.Entry<String, Object> e : variables.entrySet()) {
            ew1.put(e.getKey(), e.getValue());
          }
        }
        if(type != null) ew1.put("type", type.toString());
      });
    }

