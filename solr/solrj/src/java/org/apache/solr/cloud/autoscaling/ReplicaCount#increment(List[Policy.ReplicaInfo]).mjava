  public void increment(List<Policy.ReplicaInfo> infos) {
    if (infos == null) return;
    for (Policy.ReplicaInfo info : infos) {
      switch (info.type) {
        case NRT:
          nrt++;
          break;
        case PULL:
          pull++;
          break;
        case TLOG:
          tlog++;
          break;
        default:
          nrt++;
      }
    }
  }

