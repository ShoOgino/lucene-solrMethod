    @Override
    public boolean equals(Object that) {
      if (that instanceof Violation) {
        Violation ve = (Violation) that;
        return Objects.equals(this.shard, (ve).shard) &&
            Objects.equals(this.coll, (ve).coll) &&
            Objects.equals(this.node, (ve).node);
      }
      return false;
    }

