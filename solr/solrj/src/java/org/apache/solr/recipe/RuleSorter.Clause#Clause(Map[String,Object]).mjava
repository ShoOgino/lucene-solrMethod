    Clause(Map<String, Object> m) {
      conditions = m.entrySet().stream()
          .filter(e -> !"strict".equals(e.getKey().trim()))
          .map(Condition::new)
          .collect(toList());
      Object o = m.get("strict");
      if (o == null) return;
      strict = o instanceof Boolean ? (Boolean) o : Boolean.parseBoolean(o.toString());
    }

