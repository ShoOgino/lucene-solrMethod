  /**
   * A main class that starts jetty+solr 
   * This is useful for debugging
   */
  public static void main( String[] args )
  {
    try {
      JettySolrRunner jetty = new JettySolrRunner( "/solr", 3456 );
      jetty.start();
    }
    catch( Exception ex ) {
      ex.printStackTrace();
    }
  }

