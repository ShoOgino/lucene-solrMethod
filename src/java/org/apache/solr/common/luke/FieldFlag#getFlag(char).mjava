    public static FieldFlag getFlag(char abbrev){
      FieldFlag result = null;
      FieldFlag [] vals = FieldFlag.values();
      for (int i = 0; i < vals.length; i++) {
         if (vals[i].getAbbreviation() == abbrev){
           result = vals[i];
           break;
         }
      }
      return result;
    }

