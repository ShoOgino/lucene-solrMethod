  public void addDoc(URL doc)
  {
    if (doc == null) {
      // should throw runtime exception
      return;
    }
    if (docs != null) {
      URL[] newDocs = new URL[docs.length+1];
      int i;
      for (i = 0; i < docs.length; i++) {
        newDocs[i] = docs[i];
      }
      newDocs[i] = doc;
      docs = newDocs;
    } else {
      docs = new URL[1];
      docs[0] = doc;
    }
  }

