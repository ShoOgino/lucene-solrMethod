  private boolean equals(char[] text1, int len, char[] text2) {
    if (len != text2.length)
      return false;
    for(int i=0;i<len;i++) {
      if (ignoreCase) {
        if (Character.toLowerCase(text1[i]) != text2[i])
          return false;
      } else {
        if (text1[i] != text2[i])
          return false;
      }
    }
    return true;
  }

