  public String toString() {
    StringBuffer sb = new StringBuffer();
    sb.append('(');
    
    if (hasAttributes()) {
      // TODO Java 1.5
      //Iterator<Attribute> it = attributes.values().iterator();
      Iterator it = getAttributesIterator();
      if (it.hasNext()) {
        sb.append(it.next().toString());
      }
      while (it.hasNext()) {
        sb.append(',');
        sb.append(it.next().toString());
      }
    }
    sb.append(')');
    return sb.toString();
  }

