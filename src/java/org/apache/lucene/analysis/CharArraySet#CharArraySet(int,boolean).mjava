  /** Create set with enough capacity to hold startSize
   *  terms */
  public CharArraySet(int startSize, boolean ignoreCase) {
    this.ignoreCase = ignoreCase;
    int size = INIT_SIZE;
    while(((double) startSize)/size >= MAX_LOAD_FACTOR)
      size *= 2;
    mask = size-1;
    entries = new char[size][];
  }

