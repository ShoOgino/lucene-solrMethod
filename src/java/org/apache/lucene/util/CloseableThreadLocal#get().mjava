  public Object get() {
    WeakReference weakRef = (WeakReference) t.get();
    if (weakRef == null) {
      Object iv = initialValue();
      if (iv != null) {
        set(iv);
        return iv;
      } else
        return null;
    } else {
      Object v = weakRef.get();
      // This can never be null, because we hold a hard
      // reference to the underlying object:
      assert v != null;
      return v;
    }
  }

