  private boolean atMatch() {
    if (min().doc() == max.doc()) {               // at a match?
      int matchLength = max.end() - min().start();
      if (((matchLength - totalLength) <= slop)   // check slop
          && (!inOrder || matchIsOrdered())) {    // check order
        return true;
      }
    }
    return false;
  }

