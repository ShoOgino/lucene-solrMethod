  protected void doUndeleteAll() {
    deletedDocsDirty = false;
    undeleteAll = true;
    if (deletedDocs != null) {
      assert deletedDocsRef != null;
      deletedDocsRef.decRef();
      deletedDocs = null;
      deletedDocsRef = null;
    } else {
      assert deletedDocsRef == null;
    }
  }

