  protected void doClose() throws IOException {
    termVectorsLocal.close();
    fieldsReaderLocal.close();
    
    if (deletedDocs != null) {
      deletedDocsRef.decRef();
      // null so if an app hangs on to us we still free most ram
      deletedDocs = null;
    }

    Iterator it = norms.values().iterator();
    while (it.hasNext()) {
      ((Norm) it.next()).decRef();
    }
    if (core != null) {
      core.decRef();
    }
  }

