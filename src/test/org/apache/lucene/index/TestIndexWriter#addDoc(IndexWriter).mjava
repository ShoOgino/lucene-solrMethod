    private void addDoc(IndexWriter writer)
    {
        Document doc = new Document();

        doc.add(Field.Keyword("id","id" + docCount));
        doc.add(Field.UnStored("content","aaa"));

        try {
          writer.addDocument(doc);
        }
        catch (IOException e) {
          e.printStackTrace();
        }
        docCount++;
    }

