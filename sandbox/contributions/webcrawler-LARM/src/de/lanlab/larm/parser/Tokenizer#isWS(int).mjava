    /**
     * Gets the wS attribute of the Tokenizer object
     *
     * @param c  Description of the Parameter
     * @return   The wS value
     */
    public boolean isWS(int c)
    {
        switch (c)
        {
            case ' ':
            case '\t':
            case '\r':
            case '\n':
                return true;
            default:
                return false;
        }
    }

