    /**
     * A unit test for JUnit
     *
     * @return   Description of the Return Value
     */
    public static boolean testInsert()
    {
        CachingQueue cq = new CachingQueue("testQueue2", 10);
        String test = "Test1";
        affirm(cq.size() == 0);
        cq.insert(test);
        affirm(cq.size() == 1);
        return (cq.remove() == test);
    }

