	String getLinkedClass(String name, ClassDoc cls, String dstpath) {
		if (cls.isIncluded()) {
			StringBuffer sb = new StringBuffer();
			sb.append("<a href=");
			sb.append(dstpath + getPath(cls.containingPackage()) + PREFIX + cls.name() + ".html>");
			sb.append(name);
			sb.append("</a>");
			return sb.toString();
		}
		return name;
	}

