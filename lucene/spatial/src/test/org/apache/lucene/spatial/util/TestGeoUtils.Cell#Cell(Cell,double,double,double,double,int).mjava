    public Cell(Cell parent,
                double minLon, double minLat,
                double maxLon, double maxLat,
                int splitCount) {
      assert maxLon >= minLon;
      assert maxLat >= minLat;
      this.parent = parent;
      this.minLon = minLon;
      this.minLat = minLat;
      this.maxLon = maxLon;
      this.maxLat = maxLat;
      this.cellID = nextCellID++;
      this.splitCount = splitCount;
    }

