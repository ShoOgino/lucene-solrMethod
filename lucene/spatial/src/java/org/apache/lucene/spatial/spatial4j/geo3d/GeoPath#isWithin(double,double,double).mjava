    @Override
    public boolean isWithin(final double x, final double y, final double z)
    {
        for (SegmentEndpoint pathPoint : points) {
            if (pathPoint.isWithin(x,y,z))
                return true;
        }
        for (PathSegment pathSegment : segments) {
            if (pathSegment.isWithin(x,y,z))
                return true;
        }
        return false;
    }

