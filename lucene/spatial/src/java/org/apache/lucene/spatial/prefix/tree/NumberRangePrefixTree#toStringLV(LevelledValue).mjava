  protected String toStringLV(LevelledValue lv) {
    StringBuilder buf = new StringBuilder();
    buf.append('[');
    for (int level = 1; level <= lv.getLevel(); level++) {
      buf.append(lv.getValAtLevel(level)).append(',');
    }
    buf.setLength(buf.length()-1);//chop off ','
    buf.append(']');
    return buf.toString();
  }

