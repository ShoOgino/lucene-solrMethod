    protected static boolean meetsAllBounds(final GeoPoint p, final Membership[] bounds, final Membership[] moreBounds) {
        for (final Membership bound : bounds) {
            if (!bound.isWithin(p))
                return false;
        }
        for (final Membership bound : moreBounds) {
            if (!bound.isWithin(p))
                return false;
        }
        return true;
    }

