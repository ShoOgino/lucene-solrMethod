  private int numberOfCorners(double ax, double ay, double bx, double by, double cx, double cy) {
    int containsCount = 0;
    if (contains(ax, ay)) {
      containsCount++;
    }
    if (contains(bx, by)) {
      containsCount++;
    }
    if (contains(cx, cy)) {
      containsCount++;
    }
    return containsCount;
  }

