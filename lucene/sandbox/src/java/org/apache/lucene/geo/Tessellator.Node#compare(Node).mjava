    /** compare nodes by y then x */
    public int compare(Node other) {
      if (this.getLat() > other.getLat()) {
        return 1;
      } else if (this.getLat() == other.getLat()) {
        if (this.getLon() > other.getLon()) {
          return 1;
        } else if (this.getLon() == other.getLon()) {
          return 0;
        }
      }
      return -1;
    }

