    private int numTermsAtSamePosition(int[] positions, int i) {
      int numTermsAtSamePosition = 1;
      for (int j = i + 1; j < positions.length; ++j) {
        if (positions[j] == positions[i]) {
          ++numTermsAtSamePosition;
        }
      }
      return numTermsAtSamePosition;
    }

