  @Override
  public boolean isWithin(final double x, final double y, final double z) {
    if (!localIsWithin(x, y, z)) {
      return false;
    }
    if (holes != null) {
      for (final GeoPolygon polygon : holes) {
        if (polygon.isWithin(x, y, z)) {
          return false;
        }
      }
    }
    return true;
  }

