    public QueryState(IndexInput in, int maxDoc,
                      int xMin, int xMax,
                      int yMin, int yMax,
                      int zMin, int zMax,
                      ValueFilter valueFilter) {
      this.in = in;
      this.docs = new DocIdSetBuilder(maxDoc);
      this.xMin = xMin;
      this.xMax = xMax;
      this.yMin = yMin;
      this.yMax = yMax;
      this.zMin = zMin;
      this.zMax = zMax;
      this.valueFilter = valueFilter;
    }

