  @Override
  public void setSortableFields(Collection<String> sortableFields) {
    fieldCombo1.removeAllItems();
    fieldCombo2.removeAllItems();

    fieldCombo1.addItem("");
    fieldCombo2.addItem("");

    for (String field : sortableFields) {
      fieldCombo1.addItem(field);
      fieldCombo2.addItem(field);
    }
  }

