  /**
   * @see #closeSafely(boolean, Closeable...)
   */
  public static void closeWhileHandlingException(Iterable<? extends Closeable> objects) throws IOException {
    for (Closeable object : objects) {
      try {
        if (object != null) {
          object.close();
        }
      } catch (Throwable t) {
      }
    }
  }

