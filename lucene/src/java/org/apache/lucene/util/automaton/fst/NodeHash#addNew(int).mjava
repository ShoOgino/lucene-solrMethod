  // called only by rehash
  private void addNew(int address) throws IOException {
    final int h = hash(address);
    int h2 = h;
    int c = 1;
    while(true) {
      final int pos = h2 & mask;
      if (table[pos] == 0) {
        table[pos] = address;
        break;
      }

      // quadratic probe
      h2 = h + (c + c*c)/2;
      c++;
      conf++;
    }
  }

