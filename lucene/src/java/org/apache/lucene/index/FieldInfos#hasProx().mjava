  /** Returns true if any fields do not omitTermFreqAndPositions */
  public boolean hasProx() {
    if (isReadOnly()) {
      return hasProx;
    }
    // mutable FIs must check!
    for (FieldInfo fi : this) {
      if (fi.isIndexed && !fi.omitTermFreqAndPositions) {
        return true;
      }
    }
    return false;
  }

