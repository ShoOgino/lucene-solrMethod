    @Override
    void write(IndexOutput output, int numDocs) throws IOException {
      assert maxDocID + 1 <= numDocs;
      output.writeInt(numDocs);
      output.writeBytes(values, 0, maxDocID + 1);
      final byte zero = 0;
      for (int i = maxDocID + 1; i < numDocs; i++) {
        output.writeByte(zero);
      }
    }

