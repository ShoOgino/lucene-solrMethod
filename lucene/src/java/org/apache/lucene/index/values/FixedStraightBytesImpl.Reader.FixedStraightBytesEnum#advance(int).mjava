      @Override
      public int advance(int target) throws IOException {
        if(target >= maxDoc){
          ref.length = 0;
          ref.offset = 0;
          return pos = NO_MORE_DOCS;
        }
        if((target-1) != pos) // pos inc == 1
          datIn.seek(fp + target * size);
        datIn.readBytes(ref.bytes, 0, size);
        return pos = target;
      }

