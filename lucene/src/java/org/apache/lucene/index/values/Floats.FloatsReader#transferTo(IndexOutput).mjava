    int transferTo(IndexOutput out) throws IOException {
      IndexInput indexInput = (IndexInput) datIn.clone();
      try {
        indexInput.seek(CodecUtil.headerLength(CODEC_NAME));
        // skip precision:
        indexInput.readByte();
        out.copyBytes(indexInput, precisionBytes * maxDoc);
      } finally {
        indexInput.close();
      }
      return maxDoc;
    }

