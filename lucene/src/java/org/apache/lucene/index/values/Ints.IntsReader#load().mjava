    @Override
    public Source load() throws IOException {
      boolean success = false;
      IndexInput input = null;
      try {
        input = cloneData();
        final Source source = arrayTemplate.newFromInput(input, maxDoc);
        success = true;
        return source;
      } finally {
        if (!success) {
          IOUtils.closeWhileHandlingException(input, datIn);
        }
      }
    }

