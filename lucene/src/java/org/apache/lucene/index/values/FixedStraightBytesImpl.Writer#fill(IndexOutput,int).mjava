    // Fills up to but not including this docID
    private void fill(IndexOutput datOut, int docID) throws IOException {
      assert size >= 0;
      final long numBytes = (docID - (lastDocID+1))*size;
      final byte zero = 0;
      for (long i = 0; i < numBytes; i++) {
        datOut.writeByte(zero);
      }
    }

