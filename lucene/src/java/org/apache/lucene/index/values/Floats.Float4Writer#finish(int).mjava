    @Override
    synchronized public void finish(int docCount) throws IOException {
      if (datOut == null)
        return; // no data added - don't create file!
      if (docCount > lastDocId + 1)
        for (int i = lastDocId; i < docCount; i++) {
          datOut.writeInt(INT_ZERO); // default value
        }
      datOut.close();
    }

