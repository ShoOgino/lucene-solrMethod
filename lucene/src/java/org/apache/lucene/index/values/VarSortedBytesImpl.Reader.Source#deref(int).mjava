      // ord is 0-based
      @Override
      protected BytesRef deref(int ord) {
        
        final long nextOffset;
        if (ord == valueCount - 1) {
          nextOffset = totBytes;
        } else {
          nextOffset = ordToOffsetIndex.get(1 + ord);
        }
        final long offset = ordToOffsetIndex.get(ord);
        data.fill(bytesRef, offset , (int)(nextOffset - offset));
        return bytesRef;
      }

