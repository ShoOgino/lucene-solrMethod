    @Override
    public void add(int docID, BytesRef bytes) throws IOException {
      if (bytes.length == 0) { // default value - skip it
        return;
      }
      checkSize(bytes);
      fillDefault(docID);
      int ord = hash.add(bytes);
      if (ord < 0) {
        ord = (-ord) - 1;
      }
      
      docToEntry[docID] = ord;
      lastDocId = docID;
    }

