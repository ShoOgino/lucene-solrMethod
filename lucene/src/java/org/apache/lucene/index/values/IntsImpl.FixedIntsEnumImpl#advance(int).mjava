    @Override
    public int advance(int target) throws IOException {
      if (target >= maxDoc) {
        return pos = NO_MORE_DOCS;
      }
      assert target > pos;
      if (target > pos+1) {
        dataIn.seek(dataIn.getFilePointer() + ((target - pos - 1) * 8));
      }
      intsRef.ints[intsRef.offset] = dataIn.readLong();
      return pos = target;
    }

