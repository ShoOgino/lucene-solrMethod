    @Override
    public int advance(int target) throws IOException {
      if (target >= maxDoc)
        return pos = NO_MORE_DOCS;
      dataIn.seek(fp + (target * precision));
      ref.floats[0] = Double.longBitsToDouble(dataIn.readLong());
      ref.offset = 0; // nocommit -- can we igore this?
      return pos = target;
    }

