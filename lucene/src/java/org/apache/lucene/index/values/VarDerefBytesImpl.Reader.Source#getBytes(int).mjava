      @Override
      public BytesRef getBytes(int docID) {
        long address =  index.get(docID);
        if (address == 0) {
          assert defaultValue.length == 0: " default value manipulated";
          return defaultValue;
        } else {
          data.fillUsingLengthPrefix2(bytesRef, --address);
          return bytesRef;
        }
      }

