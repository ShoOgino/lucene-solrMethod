      @Override
      public void updateDocHead(int doc) throws IOException {
        for (int i = 0; i < sortsIndex.length; i++) {
          sortOrds[i] = sortsIndex[i].getOrd(doc);
          sortValues[i].copyBytes(sortsIndex[i].get(doc));
        }
        this.doc = doc + readerContext.docBase;
      }

