    @Override
    public long seekBlock(BytesRef term) {
      state = STATE_SEEK;
      try {
        BytesRefFSTEnum.InputOutput<Long> seekFloor = fstEnum.seekFloor(term);
        if (seekFloor == null) {
          blockFilePointer = -1;
        } else {
          blockFilePointer = seekFloor.output;
        }
        return blockFilePointer;
      } catch (IOException e) {
        // Should never happen.
        throw new RuntimeException(e);
      }
    }

