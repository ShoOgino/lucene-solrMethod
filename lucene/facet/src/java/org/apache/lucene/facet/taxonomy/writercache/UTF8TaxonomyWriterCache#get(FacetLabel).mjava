  @Override
  public int get(FacetLabel label) {
    BytesRef bytes = toBytes(label);
    int id;
    synchronized (this) {
      id = map.find(bytes);
    }
    if (id == -1) {
      return LabelToOrdinal.INVALID_ORDINAL;
    }
    int page = id / ORDINALS_PAGE_SIZE;
    int offset = id % ORDINALS_PAGE_MASK;
    return ordinals[page][offset];
  }

