  @Override
  public int get(CategoryPath categoryPath, int length) {
    if (length < 0 || length > categoryPath.length()) {
      length = categoryPath.length();
    }
    lock.readLock().lock();
    try {
      return cache.getOrdinal(categoryPath, length);
    } finally {
      lock.readLock().unlock();
    }
  }

