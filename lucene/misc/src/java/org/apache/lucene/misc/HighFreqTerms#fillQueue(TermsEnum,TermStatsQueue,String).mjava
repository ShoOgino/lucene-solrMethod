  public static void fillQueue(TermsEnum termsEnum, TermStatsQueue tiq, String field) throws Exception {
    
  while (true) {
      BytesRef term = termsEnum.next();
      if (term != null) {
        tiq.insertWithOverflow(new TermStats(field, term, termsEnum.docFreq()));
      } else {
        break;
      }
    }
  }

