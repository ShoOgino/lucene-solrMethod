    @Override
    protected void setMax(int dim, Object val) {
      InetAddress v = (InetAddress)val;
      byte[] e = InetAddressPoint.encode(v);

      if (StringHelper.compare(e.length, max, 0, e, 0) > 0) {
        min = e;
        minAddress = v;
      } else {
        max = e;
        maxAddress = v;
      }
    }

