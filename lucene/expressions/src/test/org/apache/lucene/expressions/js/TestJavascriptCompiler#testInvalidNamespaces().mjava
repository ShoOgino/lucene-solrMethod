    try {
      JavascriptCompiler.compile("object..invalid");
      fail();
    }
    catch (ParseException expected) {
      //expected
    }

    try {
      JavascriptCompiler.compile(".invalid");
      fail();
    }
    catch (ParseException expected) {
      //expected
    }
  }

