  @Override
  public BytesRef next() {
    upto++;
    if (upto >= terms.length) {
      return null;
    }
    if (terms[upto].field() == field) {
      br.copy(terms[upto].getTerm().text());
      return br;
    } else {
      // make sure field was interned
      assert !terms[upto].field().equals(field);
      return null;
    }
  }

