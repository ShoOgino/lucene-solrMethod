  public TermsEnum getTermsEnum(String termText) throws IOException {
    Terms terms = MultiFields.getTerms(getIndexReader(), getFieldName());
    if (terms != null) {
      TermsEnum termsEnum = terms.iterator();
      termsEnum.seek(new BytesRef(termText));
      return termsEnum;
    } else {
      return null;
    }
  }

