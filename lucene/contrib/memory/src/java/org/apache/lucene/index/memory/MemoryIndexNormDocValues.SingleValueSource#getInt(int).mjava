    @Override
    public long getInt(int docID) {
      switch (type) {
      case FIXED_INTS_16:
      case FIXED_INTS_32:
      case FIXED_INTS_64:
      case FIXED_INTS_8:
      case VAR_INTS:
        assert numericValue != null;
        return numericValue.longValue();
      }
      return super.getInt(docID);
    }

