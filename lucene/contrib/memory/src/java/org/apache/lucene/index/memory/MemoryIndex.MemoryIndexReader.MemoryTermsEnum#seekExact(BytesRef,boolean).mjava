      @Override
      public boolean seekExact(BytesRef text, boolean useCache) {
        termUpto = Arrays.binarySearch(info.sortedTerms, text, termComparator);
        if (termUpto >= 0) {
          br.copy(info.sortedTerms[termUpto].getKey());
          return true;
        } else {
          return false;
        }
      }

