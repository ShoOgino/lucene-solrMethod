  public void testSeemsNonEmptyButIsNot2() throws Exception {
    LightAutomaton a = new LightAutomaton();
    int init = a.createState();
    int s = a.createState();
    a.addTransition(init, s, 'a');
    // An orphan'd accept state
    s = a.createState();
    a.setAccept(s, true);
    a.finishState();
    assertTrue(BasicOperations.isEmpty(a));
  }

