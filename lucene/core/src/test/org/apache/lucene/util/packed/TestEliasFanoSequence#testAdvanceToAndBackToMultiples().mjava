  public void testAdvanceToAndBackToMultiples() {
    for (int s = 2; s < 130; s++) {
      long[] values = new long[s];
      for (int i = 0; i < s; i++) {
        values[i] = i * ((long) i + 1) / 2; // Add a gap of s to previous
        // s = (s*(s+1) - (s-1)*s)/2
      }
      tstEFSadvanceToAndBackToMultiples(values, values[s-1], 10);
    }
  }

