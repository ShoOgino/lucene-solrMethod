  public void testStrictMonotoneSequences() {
    for (int s = 2; s < 1222; s++) {
      long[] values = new long[s];
      for (int i = 0; i < s; i++) {
        values[i] = i * ((long) i - 1) / 2; // Add a gap of (s-1) to previous
        // s = (s*(s+1) - (s-1)*s)/2
      }
      tstEFS2(values);
    }
  }

