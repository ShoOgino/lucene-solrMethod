  public boolean cmp(int[] a, int sza, int[] b, int szb) {
    if (sza!=szb)
      return false;
    for (int i=0; i<sza; ++i) {
      if (a[i]!=b[i]) {
        System.err.println(String.format(Locale.ENGLISH, "! %08x != %08x in %d",a[i],b[i],i));
        return false;
      }
    }
    return true;
  }

