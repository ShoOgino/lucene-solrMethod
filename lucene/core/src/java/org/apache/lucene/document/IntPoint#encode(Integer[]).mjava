  /** Encode n-dimensional integer values into binary encoding */
  public static byte[][] encode(Integer value[]) {
    byte[][] encoded = new byte[value.length][];
    for (int i = 0; i < value.length; i++) {
      if (value[i] != null) {
        encoded[i] = encodeDimension(value[i]);
      }
    }
    return encoded;
  }

