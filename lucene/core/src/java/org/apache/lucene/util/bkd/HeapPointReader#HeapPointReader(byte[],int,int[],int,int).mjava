  public HeapPointReader(byte[] block, int packedBytesLength, int[] docIDs, int start, int end) {
    this.block = block;
    this.docIDs = docIDs;
    curRead = start-1;
    this.end = end;
    this.packedBytesLength = packedBytesLength;
    this.pointValue = new HeapPointValue(block, packedBytesLength);
  }

