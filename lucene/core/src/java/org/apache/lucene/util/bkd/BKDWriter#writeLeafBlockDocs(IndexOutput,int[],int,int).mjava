  protected void writeLeafBlockDocs(IndexOutput out, int[] docIDs, int start, int count) throws IOException {
    out.writeVInt(count);

    int lastDocID = 0;
    for (int i=0;i<count;i++) {
      int docID = docIDs[start + i];
      out.writeVInt(docID - lastDocID);
      lastDocID = docID;
    }
  }

