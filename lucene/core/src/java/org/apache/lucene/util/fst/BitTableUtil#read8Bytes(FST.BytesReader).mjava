  private static long read8Bytes(FST.BytesReader reader) throws IOException {
    return readByte(reader)
        | readByte(reader) << 8
        | readByte(reader) << 16
        | readByte(reader) << 24
        | readByte(reader) << 32
        | readByte(reader) << 40
        | readByte(reader) << 48
        | readByte(reader) << 56;
  }

