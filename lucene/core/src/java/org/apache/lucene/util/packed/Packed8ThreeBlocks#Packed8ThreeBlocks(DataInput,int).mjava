  Packed8ThreeBlocks(DataInput in, int valueCount) throws IOException {
    this(valueCount);
    for (int i = 0; i < blocks.length; i++) {
      blocks[i] = in.readByte();
    }
    final int mod = blocks.length % 8;
    if (mod != 0) {
      final int pad = 8 - mod;
      // round out long
      for (int i = 0; i < pad; i++) {
        in.readByte();
      }
    }
  }

