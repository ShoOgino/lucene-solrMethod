    @Override
    public void seekExact(long ord) throws IOException {
      if (ord < -1 || ord >= numTerms) {
        throw new IOException("ord is out of range: ord=" + ord + ", numTerms=" + numTerms);
      }
      if (ord < this.ord) {
        reset();
      }
      for (int i = this.ord; i < ord; ++i) {
        next();
      }
      assert ord == this.ord();
    }

