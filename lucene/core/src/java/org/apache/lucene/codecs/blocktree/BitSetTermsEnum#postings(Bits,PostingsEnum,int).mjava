  @Override
  public PostingsEnum postings(Bits liveDocs, PostingsEnum reuse, int flags) {
    if (flags != PostingsEnum.NONE) {
      // We only work with DOCS_ONLY fields
      return null;
    }
    if (liveDocs != null) {
      throw new IllegalArgumentException("cannot handle live docs");
    }
    postingsEnum.reset();
    return postingsEnum;
  }

