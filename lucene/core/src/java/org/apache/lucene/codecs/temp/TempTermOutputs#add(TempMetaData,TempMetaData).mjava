      return t2;
    } else if (t2 == NO_OUTPUT) {
      if (DEBUG) System.out.println("ret:"+t1);
      return t1;
    }
    assert t1.longs != null;
    assert t2.longs != null;

    int pos = 0;
    long[] accum = new long[longsSize];  // nocommit: reuse
    while (pos < longsSize) {
      accum[pos] = t1.longs[pos] + t2.longs[pos];
      assert(accum[pos] >= 0);
      pos++;
    }
    TempMetaData ret;
