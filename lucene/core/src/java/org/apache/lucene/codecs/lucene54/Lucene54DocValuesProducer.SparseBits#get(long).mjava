    boolean get(final long docId) {
      if (docId < this.docId) {
        // reading doc IDs backward, go back to the start
        reset();
      }

      if (docId >= nextDocId) {
        exponentialSearch(docId);
      }

      assert checkInvariants(index + 1, docId);
      return docId == this.docId;
    }

