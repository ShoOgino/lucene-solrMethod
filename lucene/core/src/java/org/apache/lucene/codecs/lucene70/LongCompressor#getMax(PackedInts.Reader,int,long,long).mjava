  private static long getMax(final PackedInts.Reader values, final int length, final long min, final long gcd) {
    long rawMax = Long.MIN_VALUE;
    for (int i = 0 ; i < length ; i++) {
      long value = values.get(i);
      if (value > rawMax) {
        rawMax = value;
      }
    }
    return (rawMax-min)/gcd;
  }

