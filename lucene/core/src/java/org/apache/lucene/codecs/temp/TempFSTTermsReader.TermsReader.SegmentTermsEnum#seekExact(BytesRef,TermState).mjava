      @Override
      public void seekExact(BytesRef target, TermState otherState) {
        if (term == null || target.compareTo(term) != 0) {
          state.copyFrom(otherState);
          term = BytesRef.deepCopyOf(target);
          seekPending = true;
        }
      }

