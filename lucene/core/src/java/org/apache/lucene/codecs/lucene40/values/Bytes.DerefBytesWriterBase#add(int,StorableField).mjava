    @Override
    public void add(int docID, StorableField value) throws IOException {
      BytesRef bytes = value.binaryValue();
      assert bytes != null;
      if (bytes.length == 0) { // default value - skip it
        return;
      }
      checkSize(bytes);
      fillDefault(docID);
      int ord = hash.add(bytes);
      if (ord < 0) {
        ord = (-ord) - 1;
      } else {
        maxBytes += bytes.length;
      }
      
      
      docToEntry[docID] = ord;
      lastDocId = docID;
    }

