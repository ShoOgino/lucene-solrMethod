      public BytesRef nextTerm() {
        while (ord < source.getValueCount()) {
          ord++;
          if (liveTerms == null || liveTerms.get(ord)) {
            source.getByOrd(ord, scratch);
            return scratch;
          } else {
            // Skip "deleted" terms (ie, terms that were not
            // referenced by any live docs):
            ord++;
          }
        }

        return null;
      }

