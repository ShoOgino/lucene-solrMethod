    @Override
    public int nextDoc() {
      while (true) {
        docID++;
        if (docID == ords.length) {
          docID = NO_MORE_DOCS;
          break;
        }
        if (ords[docID] != null) {
          break;
        }
        // skip missing docs
      }
      ordUpto = 0;
      return docID;
    }

