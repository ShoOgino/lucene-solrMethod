    @Override
    public int nextDoc() {
      if (idx >= size) {
        return doc = DocIdSetIterator.NO_MORE_DOCS;
      }
      doc = (int) docs.get(idx);
      ++idx;
      while (idx < size && docs.get(idx) == doc) {
        // scan forward to last update to this doc
        ++idx;
      }
      // idx points to the "next" element
      value = values.get(idx - 1);
      return doc;
    }

