    @Override
    public OrdIterator getOrds(int docID, OrdIterator reuse) {
      MultiOrdIterator iterator;
      if (reuse instanceof MultiOrdIterator) {
        iterator = (MultiOrdIterator) reuse;
      } else {
        iterator = new MultiOrdIterator();
      }
      iterator.reset(docID);
      return iterator;
    }

