  /** Returns true if any fields have positions */
  public boolean hasProx() {
    if (isReadOnly()) {
      return hasProx;
    }
    // mutable FIs must check!
    for (FieldInfo fi : this) {
      if (fi.isIndexed && fi.indexOptions.compareTo(IndexOptions.DOCS_AND_FREQS_AND_POSITIONS) >= 0) {
        return true;
      }
    }
    return false;
  }

