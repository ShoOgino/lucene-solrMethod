    @Override
    public int nextDoc() {
      while (true) {
        docID++;
        if (docID == values.length) {
          docID = NO_MORE_DOCS;
          break;
        }
        if (values[docID] != null) {
          break;
        }
        // skip missing docs
      }
      upto = 0;
      return docID;
    }

