      @Override
      public void copy(int slot, int doc) {
        final int ord = termsIndex.getOrd(doc);
        ords[slot] = ord;
        if (ord == -1) {
          values[slot] = null;
        } else {
          assert ord >= 0;
          if (values[slot] == null) {
            values[slot] = new BytesRef();
          }
          termsIndex.lookupOrd(ord, values[slot]);
        }
        readerGen[slot] = currentReaderGen;
      }

