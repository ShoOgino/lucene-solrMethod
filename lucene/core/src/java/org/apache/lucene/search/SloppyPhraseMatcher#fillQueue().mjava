  /** Fill the queue (all pps are already placed */
  private void fillQueue() {
    pq.clear();
    for (PhrasePositions pp : phrasePositions) {  // iterate cyclic list: done once handled max
      if (pp.position > end) {
        end = pp.position;
        advanceEndPostings = pp.ord;
      }
      if (pp.position == end) {
        if (pp.ord > advanceEndPostings) {
          advanceEndPostings = pp.ord;
        }
      }
      pq.add(pp);
    }
  }

