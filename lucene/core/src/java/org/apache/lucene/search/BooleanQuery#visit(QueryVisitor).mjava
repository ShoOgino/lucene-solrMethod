  @Override
  public void visit(QueryVisitor visitor) {
    for (BooleanClause.Occur occur : clauseSets.keySet()) {
      if (clauseSets.get(occur).size() > 0) {
        QueryVisitor v = visitor.getSubVisitor(occur, this);
        for (Query q : clauseSets.get(occur)) {
          q.visit(v);
        }
      }
    }
  }

