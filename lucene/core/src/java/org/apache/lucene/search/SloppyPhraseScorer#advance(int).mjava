  @Override
  public int advance(int target) throws IOException {
    assert target > docID();
    int doc;
    for (doc = conjunction.advance(target); doc != NO_MORE_DOCS; doc = conjunction.nextDoc()) {
      sloppyFreq = phraseFreq(); // check for phrase
      if (sloppyFreq != 0f) {
        break;
      }
    }

    return doc;
  }

