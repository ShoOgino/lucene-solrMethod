    int freq() throws IOException {
      DisiWrapper w = queue.topList();
      int freq = ((PostingsEnum) w.iterator).freq();
      for (w = w.next; w != null; w = w.next) {
        freq += ((PostingsEnum) w.iterator).freq();
        if (freq < 0) { // overflow
          return Integer.MAX_VALUE;
        }
      }
      return freq;
    }

