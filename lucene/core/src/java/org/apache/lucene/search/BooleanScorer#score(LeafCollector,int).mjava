  @Override
  public boolean score(LeafCollector collector, int max) throws IOException {
    fakeScorer.doc = -1;
    collector.setScorer(fakeScorer);

    for (int docBase = this.max & ~MASK; docBase < max; docBase += SIZE) {
      if (scoreWindow(collector, docBase, max) == false) {
        return false;
      }
    }

    return true;
  }

