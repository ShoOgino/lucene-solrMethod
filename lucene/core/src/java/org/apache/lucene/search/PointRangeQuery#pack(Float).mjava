  private static byte[][] pack(Float value) {
    if (value == null) {
      // OK: open ended range
      return new byte[1][];
    }
    byte[][] result = new byte[][] {new byte[RamUsageEstimator.NUM_BYTES_INT]};
    NumericUtils.intToBytesDirect(NumericUtils.floatToSortableInt(value), result[0], 0);
    return result;
  }

