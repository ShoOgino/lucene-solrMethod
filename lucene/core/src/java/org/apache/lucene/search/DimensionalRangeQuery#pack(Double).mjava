  private static byte[][] pack(Double value) {
    if (value == null) {
      // OK: open ended range
      return new byte[1][];
    }
    byte[][] result = new byte[][] {new byte[RamUsageEstimator.NUM_BYTES_LONG]};
    NumericUtils.longToBytesDirect(NumericUtils.doubleToSortableLong(value), result[0], 0);
    return result;
  }

