    @Override
    public SeekStatus seekCeil(BytesRef term, boolean useCache) throws IOException {
      assert term.isValid();
      SeekStatus result = super.seekCeil(term, useCache);
      if (result == SeekStatus.END) {
        state = State.UNPOSITIONED;
      } else {
        state = State.POSITIONED;
      }
      return result;
    }

