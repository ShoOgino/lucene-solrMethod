    @Override
    public float score(float freq, long norm) throws IOException {
      // freq in bounds
      assert Float.isFinite(freq);
      assert freq > 0;
      // result in bounds
      float score = delegate.score(freq, norm);
      assert Float.isFinite(score);
      assert score <= delegate.score(freq, 1);
      assert score >= 0;
      return score;
    }

