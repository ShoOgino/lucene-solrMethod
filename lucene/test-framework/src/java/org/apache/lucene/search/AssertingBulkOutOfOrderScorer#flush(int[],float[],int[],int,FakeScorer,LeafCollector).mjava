  private static void flush(int[] docIDs, float[] scores, int[] freqs, int size,
      FakeScorer scorer, LeafCollector collector) throws IOException {
    for (int i = 0; i < size; ++i) {
      scorer.doc = docIDs[i];
      scorer.freq = freqs[i];
      scorer.score = scores[i];
      collector.collect(scorer.doc);
    }
  }

