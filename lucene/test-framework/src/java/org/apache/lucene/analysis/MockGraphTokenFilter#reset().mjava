  @Override
  public void reset() throws IOException {
    super.reset();
    end = false;
    positions.reset();
    // NOTE: must be "deterministically random" because
    // BaseTokenStreamTestCase pulls tokens twice on the
    // same input and asserts they are the same:
    this.random = new Random(seed);
    inputPos = -1;
    outputPos = 0;
    lastOutputPos = -1;
  }

