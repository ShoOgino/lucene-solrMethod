  @Override
  public MutableBits newLiveDocs(int size) throws IOException {
    assert size >= 0;
    MutableBits raw = in.newLiveDocs(size);
    assert raw != null;
    assert raw.length() == size;
    for (int i = 0; i < raw.length(); i++) {
      assert raw.get(i);
    }
    return new AssertingMutableBits(raw);
  }

