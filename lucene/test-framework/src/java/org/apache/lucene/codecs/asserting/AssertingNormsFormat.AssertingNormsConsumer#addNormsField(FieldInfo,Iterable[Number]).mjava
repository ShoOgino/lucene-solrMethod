    @Override
    public void addNormsField(FieldInfo field, Iterable<Number> values) throws IOException {
      int count = 0;
      for (Number v : values) {
        assert v != null;
        count++;
      }
      assert count == maxDoc;
      AssertingDocValuesFormat.checkIterator(values.iterator(), maxDoc, false);
      in.addNormsField(field, values);
    }

