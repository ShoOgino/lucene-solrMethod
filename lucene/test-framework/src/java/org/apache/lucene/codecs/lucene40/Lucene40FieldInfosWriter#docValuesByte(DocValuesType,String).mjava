  /** 4.0-style docvalues byte */
  public byte docValuesByte(DocValuesType type, String legacyTypeAtt) {
    if (type == null) {
      assert legacyTypeAtt == null;
      return 0;
    } else {
      assert legacyTypeAtt != null;
      return (byte) LegacyDocValuesType.valueOf(legacyTypeAtt).ordinal();
    }
  }  

