  // for testing
  int instantiatedTokenCount() {
    int tokenCount = tokenPool.size() + 1;
    if (currentShingleTokens[0] == endToken || currentShingleTokens[0] == null)
      return tokenCount;
    for (Token t = currentShingleTokens[0]; t != endToken && t != null; t = t.nextToken) {
      tokenCount++;
    }
    return tokenCount;
  }

