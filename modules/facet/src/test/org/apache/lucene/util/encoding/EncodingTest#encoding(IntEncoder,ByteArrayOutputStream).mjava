  private static void encoding(IntEncoder encoder, ByteArrayOutputStream baos) throws IOException {
    encoder.reInit(baos);
    for (int value : data) {
      encoder.encode(value);
    }
    encoder.close();

    baos.reset();
    encoder.reInit(baos);
    for (int value : data) {
      encoder.encode(value);
    }
    encoder.close();
  }

