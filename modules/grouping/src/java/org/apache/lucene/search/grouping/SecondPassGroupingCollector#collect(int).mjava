  @Override
  public void collect(int doc) throws IOException {
    final int slot = ordSet.find(index.getOrd(doc));
    //System.out.println("SP.collect doc=" + doc + " slot=" + slot);
    totalHitCount++;
    if (slot >= 0) {
      totalGroupedHitCount++;
      groupDocs[slot].collector.collect(doc);
    }
  }

