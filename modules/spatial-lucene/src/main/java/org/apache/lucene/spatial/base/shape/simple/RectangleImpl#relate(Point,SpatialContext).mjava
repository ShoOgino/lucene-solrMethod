  public SpatialRelation relate(Point point, SpatialContext ctx) {
    if (point.getY() > getMaxY() || point.getY() < getMinY() ||
        (getCrossesDateLine() ?
            (point.getX() < minX && point.getX() > maxX)
            : (point.getX() < minX || point.getX() > maxX) ))
      return SpatialRelation.DISJOINT;
    return SpatialRelation.CONTAINS;
  }

