  @Override
  public boolean equals(Object obj) {
    
    if (obj == this) return true;
    
    if (obj instanceof NumericConfig) {
      NumericConfig other = (NumericConfig) obj;
      
      if (this.precisionStep == other.precisionStep
          && this.format == other.format) {
        return true;
      }
      
    }
    
    return false;
    
  }

